   "source": [
    "def reverse(arr):\n",
    "    mid = len(arr)/2\n",
    "    for i in range(mid):\n",
    "        indx = len(arr)-i-1\n",
    "        arr[i], arr[indx] = arr[indx], arr[i]\n",
    "    return arr\n",
    "        "
   ]
