   "source": [
    "def rotate(arr, num):\n",
    "    left = arr[:num]\n",
    "    right = arr[num:]\n",
    "    left = list(reversed(left))\n",
    "    right = list(reversed(right))\n",
    "    return list(reversed(left + right))"
   ]
  },
